program Project2;

uses
  System.SysUtils;

var
  f: TextFile;
  s, sumStr: String;
  arrString: array of string;

begin
  sumStr := '';
  AssignFile(f, 'myText.txt');
  Reset(f);
  while not Eof(f) do
  begin
    readln(f, s);
    sumStr := sumStr + s;
    writeln(s);
  end;
  writeln(sumStr);
  CloseFile(f);

  readln;

end.
